{
	"compilerOptions": {
		"target": "ES2023",
		"module": "Node16",
		"moduleResolution": "Node16",
		"declaration": true,
		"strict": true,
		"verbatimModuleSyntax": true,
		"sourceMap": true,
		"forceConsistentCasingInFileNames": true,
		"rootDir": "./src",
		"outDir": "./dist",
		"composite": true
	},
	"include": ["src"]
}
